import turtle 

turtle.hideturtle()
turtle.fillcolor('blue')
turtle.forward(100)
turtle.left(120)
turtle.forward(100)
turtle.left(120)
turtle.forward(100)
turtle.left(100)
turtle.fillcolor('blue')
turtle.begin_fill()
turtle.setheading(40)
turtle.forward(65)
turtle.setheading(320)
turtle.forward(65)
turtle.end_fill() 



input('press any key to exit')
